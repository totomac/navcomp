<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainFragment">


    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.myapplication.MainFragment"
        tools:layout="@layout/main_fragment"
        android:label="MainFragment" >

        <action
            android:id="@+id/action_mainFragment_to_featurea_nav_graph"
            app:destination="@id/featurea_nav_graph" />
        <action
            android:id="@+id/action_mainFragment_to_featureb_nav_graph"
            app:destination="@id/featureb_nav_graph" />
    </fragment>
    <include app:graph="@navigation/featurea_nav_graph" />
    <include app:graph="@navigation/featureb_nav_graph" />

</navigation>
